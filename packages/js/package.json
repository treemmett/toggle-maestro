{
  "name": "toggle-maestro",
  "version": "0.1.0",
  "description": "Toggle those features, with finesse",
  "repository": "https://github.com/github:treemmett/toggle-maestro",
  "author": "Tregan <tregan@tregan.me>",
  "license": "MIT",
  "private": true,
  "main": "./lib/index.js",
  "types": "./es/src/index.d.ts",
  "unpkg": "dist/toggle-maestro.js",
  "module": "es/index.js",
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --extensions \".js,.ts,.tsx\" --out-dir lib",
    "build:es": "babel src --extensions \".js,.ts,.tsx\" --out-dir es",
    "build:umd": "cross-env NODE_ENV=development rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript -o dist/toggle-maestro.js",
    "build:umd:min": "cross-env NODE_ENV=production rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript -o dist/toggle-maestro.min.js",
    "build:types": "tsc",
    "build": "yarn build:types && yarn build:commonjs && yarn build:es && yarn build:umd && yarn build:umd:min",
    "clean": "rimraf lib dist es coverage",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.3",
    "@babel/plugin-syntax-flow": "^7.18.6",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.11.0",
    "babel-jest": "^29.2.0",
    "cross-env": "^7.0.3",
    "jest": "^29.2.0",
    "jest-environment-jsdom": "^29.2.0",
    "nock": "^13.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^29.0.3",
    "whatwg-fetch": "^3.6.2"
  },
  "peerDependencies": {
    "react": "^16.8 || ^17.0 || ^18.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.4"
  }
}
